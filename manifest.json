{
    "manifest_version": 3,
    "name": "Accessibility Tree Extension",
    "version": "1.0",
    "description": "A Chrome extension to access and display the accessibility tree.",
    "permissions": [
      "activeTab",
      "scripting",
      "debugger"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  